@using System.Data
@model C3_Dominio.Entidades.DocumentoPago
<script src="~/Bootstrap/js/jquery-1.11.0.js"></script>
@{
    DataTable dt = (DataTable)Session["TipoDocumento"];

    foreach (DataRow r in dt.Rows)
    {
        if (dt.Rows.Count > 0)
        {<table>
            <tr>
                <td><input name="txtTipoDocumento" type="text" id="txtTipoDocumento" disabled="disabled" value="@r["NombreTipo"]" /></td>
                <td>Numero Serie: </td>
                <td><input name="txtNSeries" type="text" id="txtNSeries" disabled="disabled" value="@r["NumSerie"]" /></td>
                <td>Numero Documento: </td>
                <td><input name="txtNdocumentos" type="text" id="txtNdocumentos" disabled="disabled"  value="@r["NumDocumento"]" /></td>
                <td><input name="txtNSerie" type="text" id="txtNSerie" style='visibility:hidden' value="@r["NumSerie"]" /></td>
                <td><input name="txtNdocumento" type="text" id="txtNdocumento" style='visibility:hidden' value="@r["NumDocumento"]" /></td>
            </tr>
        </table>
        }
    }
}
